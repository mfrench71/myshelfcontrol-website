{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "src/**/*.test.{ts,tsx}",
    "src/**/__tests__/**",
    "src/test/**"
  ],
  "ignoreDependencies": [
    "tailwindcss",
    "postcss"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "types": "off"
  }
}
